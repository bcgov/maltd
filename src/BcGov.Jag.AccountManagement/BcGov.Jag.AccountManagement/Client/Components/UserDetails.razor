@using BcGov.Jag.AccountManagement.Shared

<table class="table">
    <thead>
        <tr>
            <th>bcgov GUID</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Username</th>
            <th>Email</th>
            <th>UPN</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@User.Id</td>
            <td>@User.LastName</td>
            <td>@User.FirstName</td>
            <td>@User.UserName</td>
            <td>@User.Email</td>
            <td>@User.UserPrincipalName</td>
        </tr>
    </tbody>
</table>

@code {

    [EditorRequired]
    [Parameter]
    public DetailedUser User { get; set; } = default!;

}
